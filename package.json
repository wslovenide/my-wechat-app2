
{
  "name": "weconnect-app",
  "version": "1.0.5",
  "description": "WeConnect WeChat-like Mobile App",
  "main": "index.tsx",
  "scripts": {
    "start": "npx servor . index.html 8080",
    "build": "mkdir -p dist && cp index.html index.tsx manifest.json sw.js types.ts constants.tsx dist/ && cp -r components services dist/ || true",
    "cap-sync": "npm run build && npx cap sync android",
    "android-open": "npx cap open android"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "@google/genai": "^1.37.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/android": "^6.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "typescript": "^5.4.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/node": "^20.12.7"
  }
}
